<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>Servlet.SignupServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>
   
   
  
     <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    

  
  
    <servlet>
        <servlet-name>UserSignUpServlet</servlet-name>
        <servlet-class>Servlet.UserSignUpServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>UserSignUpServlet</servlet-name>
        <url-pattern>/user_signup</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
    <servlet-name>UserSignInServlet</servlet-name>
    <servlet-class>Servlet.UserSignInServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>UserSignInServlet</servlet-name>
    <url-pattern>/user_signin</url-pattern>
</servlet-mapping>
   
  
     <servlet>
        <servlet-name>FormServlet</servlet-name>
        <servlet-class>Servlet.FormServlet</servlet-class>
    </servlet>
   <servlet-mapping>
        <servlet-name>FormServlet</servlet-name>
        <url-pattern>/submitForm</url-pattern>
   </servlet-mapping>

    
    
        <servlet>
        <servlet-name>SubmitAppointmentServlet</servlet-name>
        <servlet-class>SubmitAppointmentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitAppointmentServlet</servlet-name>
        <url-pattern>/submitAppointment</url-pattern>
    </servlet-mapping>
    
    
<servlet>
    <servlet-name>DeleteAppointmentServlet</servlet-name>
    <servlet-class>Servlet.DeleteAppointmentServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>DeleteAppointmentServlet</servlet-name>
    <url-pattern>/deleteAppointment</url-pattern>
</servlet-mapping>

   <servlet>
        <servlet-name>DoctorServlet</servlet-name>
        <servlet-class>Servlet.DoctorServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DoctorServlet</servlet-name>
        <url-pattern>/submitDoctor</url-pattern>
    </servlet-mapping>


 <servlet>
        <servlet-name>patient_details</servlet-name>
        <servlet-class>Servlet.patient_details</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>patient_details</servlet-name>
        <url-pattern>/submit_patient</url-pattern>
    </servlet-mapping>
    
 </web-app>